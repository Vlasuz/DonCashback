					p Количество звезд
					.stars
						input(type="radio" id="star-11" name="rating" value="1")
						label(for="star-11")
							img(src="img/svg/ico61.svg" alt="Ico").grey-s
							img(src="img/svg/ico61-1.svg" alt="Ico").gold-s
						input(type="radio" id="star-12" name="rating" value="2")
						label(for="star-12")
							img(src="img/svg/ico61.svg" alt="Ico").grey-s
							img(src="img/svg/ico61-1.svg" alt="Ico").gold-s
