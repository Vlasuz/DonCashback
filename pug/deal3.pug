extends includes/header-singin.pug

block content

	main.page-deal
		.popup.popup-review-market
			.popup__bgd
			.popup__content
				.popup__close
					img(src="img/svg/ico47.svg" alt="Ico")
				h3 Ваше мнение очень важно!
				p Поделитесь этим ценным мнением на торговой площадке, <br />где был куплен продукт, 500 баллов будут добавлены на ваш счет после того, как отзыв будет опубликован
