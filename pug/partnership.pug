extends includes/header.pug

block content

	main.page-partnership
		.wrapper
			#breadcrumbs
				a(href="404.html")
					img(src="img/svg/ico22.svg" alt="ico")
				span Партнерство
			section.page-partnership__title
				h2 Для покупателя
				.title__p
					p Привлеченный клиент получит скидку 20% при первой покупке плана, а вы будете получать 15% с каждой покупки плана такого клиента на весь период взаимодействия клиента с сервисом.
			section.page-partnership__info
				.info__item
					.item__input.item__input_copy
						input(disabled value="B07R11LMH7")
						button Скопировать промокод
				.info__item
					span  0 / 0
					span Применили / Купили
				.info__item
					span  299 ₽
					span Всего выведено
				.info__item
					span  0 ₽
					span Для вывода
					button Вывод средств
			section.page-partnership__block
