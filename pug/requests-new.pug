extends includes/header-singin-seller.pug

block content

	main.page-requests.pages-seller
		.wrapper
			#breadcrumbs
				a(href="404.html")
					img(src="img/svg/ico22.svg" alt="ico")
				span Запросы
			.pages-seller__top.block-shadow
				.select-v2
					.select__head
						span Все статусы
					.select__body
						span Все продукты
						span В наличии
						span Нет в наличии
						span Остановлен
				form(action="#").pages-seller__search
					input(placeholder="Поиск")
					button(type="submit") Применить
				.found 132 Найдено
			section.block-shadow
				nav.block__nav
					ul
						li
							a(href="requests.html") Все Запросы
						li.li_active
							a(href="requests-new.html") Новые
						li
							a(href="404.html") Подтвержденные
						li
							a(href="404.html") Отклоненные
						li
							a(href="404.html") Отмененные покупателем
						li
							a(href="404.html") Время заказа истекло
						li
							a(href="404.html") Истекшие запросы
				.page-requests__new
					img(src="img/non-block.png" alt="Photo" width="299")
					p Здесь пока ничего нет

			.pagination
				span.pagination__num.pagination__num_current 1
				a(href="404.html").pagination__num 2
				a(href="404.html").pagination__num 3
				span.pagination__num.pagination__num_three ...
				a(href="404.html").pagination__num 7
				a(href="404.html").pagination__next
					span Следующая
					img(src="img/svg/ico23.svg" alt="Ico")
				.pagination__count.select
					.select__head
						span по 20 позиций 
					.select__body
						span по 10 позиций 
						span по 20 позиций 
						span по 30 позиций 
						span по 40 позиций






								


			
			
			


	include includes/footer-seller.pug



























	